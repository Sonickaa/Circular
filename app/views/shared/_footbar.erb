<div class="footbar">
  <div class="container-fluid">
    <div class="item item-home">
      <%= link_to "/", class: "nav-link" do %>
        <%= image_tag "icons/btn categories.svg", style: "width: 36px" %>
      <% end %>
      <%= link_to "Home", "/", class: "nav-link" %>
    </div>
    <div class="item item-wishlist">
      <%= link_to wishlist_path(current_user.wishlist), class: "nav-link" do %>
        <%= image_tag "icons/btn home.svg", style: "width: 36px" %>
      <% end %>
      <%= link_to "wishlist", wishlist_path(current_user.wishlist), class: "nav-link" %>
    </div>
    <div class="item item-addnew">
      <%= link_to "/products/new", class: "nav-link" do %>
        <%= image_tag "icons/btn add.svg", style: "width: 36px" %>
      <% end %>
      <%= link_to "add new", "/products/new", class: "nav-link" %>
    </div>
    <div class="item item-dashboard">
      <%= link_to "/dashboard", class: "nav-link" do %>
        <%= image_tag "icons/btn dashboard.svg", style: "width: 36px" %>
      <% end %>
      <%= link_to "dashboard", "/dashboard", class: "nav-link" %>
    </div>
    <div class="item item-profile">
      <%= link_to my_profile_path, class: "nav-link" do %>
        <%= image_tag "icons/btn profile.svg", style: "width: 36px" %>
      <% end %>
      <%= link_to "profile", my_profile_path, class: "nav-link" %>
    </div>
  </div>
</div>
